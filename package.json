{
    "name": "react-shoping",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "antd-mobile": "^2.2.13",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-scripts": "3.0.1",
        "zarm": "^1.1.1"
    },
    "scripts": {
        "dev": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ],
        "plugins": [
            [
                "import",
                {
                    "libraryName": "zarm",
                    "style": true
                }
            ]
        ]
    },
    "devDependencies": {
        "babel-plugin-import": "^1.11.0",
        "node-sass": "^4.12.0"
    }
}
